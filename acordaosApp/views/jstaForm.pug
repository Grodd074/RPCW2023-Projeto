extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Processo:  #[a.w3-btn.w3-round.w3-grey(href="/") #{a.Processo} ]
        
    .w3-container
        ul.w3-ul.w3-card-4
            li #[b Processo:] #{a.Processo}
            li #[b Data do Acordão:] #{a['Data do Acordão']}
            li #[b Tribunal:] #{a.tribunal}
            li #[b Relator:] #{a.Relator}
            li #[b Descritores:] #{a.Descritores.join(', ')}
            li #[b Nº Convencional:] #{a['Nº Convencional']}
            li #[b Nº do Documento:] #{a['Nº do Documento']}
            li #[b Data de Entrada:] #{a['Data de Entrada']}
            li #[b Recorrente:] #{a.Recorrente}
            li #[b Recorrido 1:] #{a['Recorrido 1']}
            li #[b Votação:] #{a.Votação}
            li #[b Texto Integral:] #{a['Texto Integral']}
            li #[b url:] #{a.url}