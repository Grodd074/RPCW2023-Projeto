extends layout

block content
  .w3-card-4
    header.w3-container.w3-purple
      h1 Editar Acord√£o

    form.w3-container(method="POST")
      fieldset
        legend Metadata
        each value, key in dados
          if key != "_id"
            label #{key}
            input.w3-input(type="text" name=key value=value)
                

      button.w3-btn.w3-purple.w3-mb-2(type="submit") Register
