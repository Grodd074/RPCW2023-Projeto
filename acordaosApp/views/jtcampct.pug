extends layout

block content
  .w3-card-4
    header.w3-container.w3-teal
      h1 Processo: #{a.Processo}
        
    .w3-container
        ul.w3-ul.w3-card-4
            li #[b Contencioso:] #{a.Contencioso}
            li #[b Peça Processual:] #{a['Peça Processual']}
            li #[b Data:] #{a.Data}
            li #[b Processo:] #{a.Processo}
            li #[b Nº Processo/TAF:] #{a['Nº Processo/TAF']}
            li #[b Sub-Secção:] #{a['Sub-Secção']}
            li #[b Magistrado:] #{a.Magistrado}
            li #[b Descritores:] #{a.Descritores.join(', ')}
            li #[b Tema:] #{a.Tema}
            li #[b url:] #[a(href="http://www.dgsi.pt/"+a.url) Link]
            li #[b Tribunal:] #{a.tribunal}
    
    footer 
        p #[a(href="/") Voltar]